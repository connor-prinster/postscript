%!

<< /PageSize [1000 1000] >> setpagedevice

/inch {20 mul} def

/hypo {
    % x = ((a - b) * cos(t)) + (a * cos( ((a - b)/b)*t ))
	% y = ((a - b) * sin(t)) + (a * sin( ((a - b)/b)*t ))
	/a exch def
	/b exch def
	/d exch def
	/t exch def

    a t div 0.9 mul
    a d div 0.7 mul
    a b div 0.5 mul 
    setrgbcolor

    d t
    moveto

	a b exch sub t cos mul % ((a - b) * cos(t))
	d a b exch sub b exch div t mul cos mul % (a * cos( ((a - b)/b)*t ))
	add % x

	a b exch sub t sin mul % ((a - b) * sin(t))
	d a b exch sub b exch div t mul sin mul % (a * sin( ((a - b)/b)*t ))
	add % y
} def

25 inch 25 inch translate

% 1.
0 2 360 {
    1 3 720 {
        newpath

        % b, d, t
		110 dup add
        dup 92 div
        20 dup div
		hypo 

        lineto stroke
	} for
    dup rotate
} for

% 2.
0 20 360
{
	1 10 720
	{
		newpath
		0 0 moveto
		dup 
		dup 720 div exch 270 div exch 0
		setrgbcolor

		%% t d littler bigr
		110 165 275
		hypo lineto 
		stroke
	} for
	rotate
} for

